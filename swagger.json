            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "refreshToken": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "token": {
                  "type": "string"
                },
                "refreshToken": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Refresh token is required"
          },
          "401": {
            "description": "Token already was used or never existed | Token expired | Token invalid | Non-existent user cannot be authorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
